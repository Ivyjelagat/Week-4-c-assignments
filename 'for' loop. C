#include <stdio.h>

int main(){
	int i;
	int start;
	int stop;
	
	printf("Enter the start value:");
	scanf("%d",&start);
	
	printf("Enter the stop value:");
	scanf("%d",&stop);
	
	
	for(int i=start; i<= stop; i ++){
		printf ("%d\n",i);
		
	}
return 0;
}
