# include <stdio.h>
int main(){
	int i;
	int start;
	int stop;
	
	
	printf("Enter the start value:");
	scanf("%d",& start);
	
	
	printf("Enter the stop value");
	scanf("%d",& stop);
	
	
	if (start>stop){
		printf("start value should be less than or equal to stop value:\n");
	
		
		printf("counting from %d to%d start; stop");
	}
		
		while (start<=stop){
			printf ("%d\n",start);
			start ++;
		}
			
return 0;
	
	
	}
