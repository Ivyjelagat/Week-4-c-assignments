#include <stdio.h>
int main (){
	int i;
	int start;
	int stop;
	
	printf("Enter the start value:");
	scanf("%d",& start);
	
	printf("Enter the  stop value:");
	scanf("%d",& stop);
	
	
	printf("numbers from %d to %d:  \n" ,start , stop);
	i= start;
	do{
		printf("%d" ,  i);
		i ++;
	}while (i<=stop);
	printf("\n");
	
	
	
return 0;

	
}
